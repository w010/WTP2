<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<formConf>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.formConf</label>
							<config>
								<type>text</type>
								<wrap>off</wrap>
								<cols>70</cols>
								<rows>40</rows>
							</config>
						</TCEforms>
					</formConf>
					<templateFile>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.templateFile</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>tmpl,html,htm</allowed>
								<max_size>100</max_size>
								<maxitems>1</maxitems>
								<size>1</size>
								<selectedListStyle>Width:180px</selectedListStyle>
							</config>
						</TCEforms>
					</templateFile>
					<redirect.after_success>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.redirect.after_success</label>
							<config>
								<type>input</type>
								<size>30</size>
								<wizards type="array">
									  <_PADDING type="integer">2</_PADDING>
									   <link type="array">
										   <type>popup</type>
										   <title>Link</title>
										   <icon>link_popup.gif</icon>
										   <script>browse_links.php?mode=wizard</script>
										   <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									  </link>
								</wizards>
							</config>
						</TCEforms>
					</redirect.after_success>
				</el>				
			</ROOT>
		</sDEF>
		<sMail>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.sheet_mail</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<sendEmailAdmin>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.sendEmailAdmin</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</sendEmailAdmin>
					<mail.recipients>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.mail.recipients</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</mail.recipients>
					<mail.subject>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.mail.subject</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</mail.subject>
					<mail.from>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.mail.from</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</mail.from>
					<mail.reply-to>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.mail.reply-to</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</mail.reply-to>
				</el>
			</ROOT>
		</sMail>
		<sDB>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.sheet_database</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<saveToDb>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.saveToDb</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</saveToDb>
					<recordStoragePid>
						<TCEforms>
							<label>LLL:EXT:w_form/locallang_db.xml:tt_content.pi_flexform.recordStoragePid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<show_thumbs>1</show_thumbs>
								<size>1</size>
								<maxitems>1</maxitems>
								<wizards>
									<edit>
										<type>popup</type>
										<title>Edit selected page</title>
										<script>wizard_edit.php</script>
										<popup_onlyOpenIfSelected>1</popup_onlyOpenIfSelected>
										<icon>edit2.gif</icon>
										<JSopenParams>height=350,width=580,status=0,menubar=0,scrollbars=1</JSopenParams>
									</edit>
								</wizards>
							</config>
						</TCEforms>
					</recordStoragePid>
				</el>
			</ROOT>
		</sDB>
		
		
