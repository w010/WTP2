<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ttnews_calendar/locallang.xml:flexform</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<templateFile>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:ttnews_calendar/locallang.xml:flexform.templateFile</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<default>EXT:ttnews_calendar/templates/default.html</default>
								<wizards type="array">
									<_PADDING>2</_PADDING>
									<link type="array">
									<type>popup</type>
									<title>Link</title>
									<icon>link_popup.gif</icon>
									<script>browse_links.php?mode=wizard&amp;act=file</script>
									<params type="array">
										<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
										<allowedExtensions>htm,html,tmpl,tpl</allowedExtensions>
									</params>
									<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</templateFile>
					<storePid>
						<TCEforms>
							<label>LLL:EXT:ttnews_calendar/locallang.xml:flexform.storePid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</storePid>
					<newsListPid>
						<TCEforms>
							<label>LLL:EXT:ttnews_calendar/locallang.xml:flexform.newsListPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</newsListPid>
					<newsSinglePid>
						<TCEforms>
							<label>LLL:EXT:ttnews_calendar/locallang.xml:flexform.newsSinglePid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</newsSinglePid>
					<imageMaxHeight>
						<TCEforms>
						<label>LLL:EXT:ttnews_calendar/locallang.xml:flexform.target</label>
						<config>
							<type>input</type>
							<size>60</size>
							<checkbox></checkbox>
						</config>
						</TCEforms>
					</imageMaxHeight>
					<dowForm>
						<TCEforms>
							<label>LLL:EXT:ttnews_calendar/locallang.xml:flexform.dowForm</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:ttnews_calendar/locallang.xml:flexform.dowForm.full</numIndex>
										<numIndex index="1">full</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:ttnews_calendar/locallang.xml:flexform.dowForm.3ltrs</numIndex>
										<numIndex index="1">3ltrs</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:ttnews_calendar/locallang.xml:flexform.dowForm.2ltrs</numIndex>
										<numIndex index="1">2ltrs</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</dowForm>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>